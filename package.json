  "repository": {
    "type": "git",
    "url": "git://github.com/hipster-labs/generator-jhipster-sample-blueprint.git"
  },
  "scripts": {
    "lint": "eslint .",
    "lint-fix": "npm run lint -- --fix",
    "pretest": "npm run lint",
    "test": "npm run test:unit -- test/*.spec.js",
    "test:unit": "mocha --timeout 20000 --slow 0 --reporter spec",
    "jsdoc": "jsdoc --configure jsdoc-conf.json",
    "release": "git push && git push --tags && npm publish",
    "release:patch": "npm version patch -a -m \"Update to %s\" && npm run release",
    "release:minor": "npm version minor -a -m \"Update to %s\" && npm run release",
    "release:major": "npm version major -a -m \"Update to %s\" && npm run release"
  },
  "dependencies": {
    "chalk": "2.4.1",
    "ejs": "2.6.1",
    "glob": "7.1.2",
    "gulp-filter": "5.1.0",
    "insight": "0.10.1",
    "jhipster-core": "3.0.2",
    "js-yaml": "3.12.0",
    "lodash": "4.17.10",
    "meow": "5.0.0",
    "mkdirp": "0.5.1",
    "pluralize": "7.0.0",
    "prettier": "1.13.5",
    "randexp": "0.4.9",
    "semver": "5.5.0",
    "shelljs": "0.8.2",
    "through2": "2.0.3",
    "generator-jhipster": "5.0.1",
    "yeoman-environment": "2.3.0",
    "yeoman-generator": "2.0.5"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "ejs-lint": "0.3.0",
